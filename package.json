	"engines": {
		"node": ">= 10.0.0"
	},
	"ava": {
		"cache": true
	},
	"scripts": {
		"back": "nodemon",
		"test": "ava --verbose ./test/*.js",
		"test:watch": "ava --watch --verbose --fail-fast ./test/*.js",
		"lint": "npx eslint --cache './lib/**/*.js'",
		"lint:watch": "esw -w --clear --color --cache './server/**/*'",
