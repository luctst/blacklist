		"style": "prettier -c './server/**/*'",
		"style:fix": "prettier --c --write './server/**/*'"
	},
	"dependencies": {
		"dotenv": "^8.2.0",
		"mongodb": "^3.5.1",
		"portfinder": "^1.0.25"
	},
	"devDependencies": {
		"ava": "^3.0.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-config-prettier": "^6.9.0",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-watch": "^6.0.1",
		"nodemon": "^2.0.2",
		"prettier": "^1.19.1"
