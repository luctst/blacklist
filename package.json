{
	"private": true,
	"name": "blacklist",
	"version": "1.0.0",
	"license": "MIT",
	"engines": {
		"node": ">= 12.0.0"
	},
	"ava": {
		"cache": true
	},
	"scripts": {
		"start": "now dev",
		"build": "react-scripts build",
		"lint": "npx eslint --cache './api/**/*'",
		"lint:watch": "esw -w --clear --color --cache './api/**/*'",
		"lint:fix": "npx eslint --fix './api/**/*'",
		"style": "prettier -c './src/**/*' && prettier -c './api/**/*'",
		"style:fix": "prettier --c --write './src/**/*' && prettier -c './api/**/*'"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"ava": "^3.0.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-config-prettier": "^6.9.0",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-watch": "^6.0.1",
		"husky": "^4.2.3",
		"now": "^17.0.4",
		"prettier": "^1.19.1",
		"react-scripts": "3.4.0"
	},
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"bcrypt": "^5.0.0",
		"cookie": "^0.4.0",
		"dotenv": "^8.2.0",
		"jsonwebtoken": "^8.5.1",
		"mongodb": "^3.5.5",
		"nodemailer": "^6.4.6",
		"react": "^16.13.0",
		"react-dom": "^16.13.0",
		"react-router-dom": "^5.1.2",
		"styled-components": "^5.0.1"
	}
}
